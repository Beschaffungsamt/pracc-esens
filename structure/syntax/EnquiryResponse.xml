<?xml version="1.0" encoding="UTF-8"?>
<Structure xmlns="urn:fdc:difi.no:2017:vefa:structure-1">
    <Term>EnquiryResponse</Term>

    <Name>Enquiry Response (T008)</Name>
    <Namespace prefix="t008">urn:oasis:names:specification:ubl:schema:xsd:EnquiryResponse-2</Namespace>
    <Namespace prefix="cbc">urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2</Namespace>
    <Namespace prefix="cac">urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2</Namespace>
    <Document>

        <Term>t008:EnquiryResponse</Term>
        <Name>Call For Tender Question And Answers, root element for Enquiry Response</Name>

        <Element>
            <Term>cbc:UBLVersionID</Term>
            <Name>UBL Version identifier</Name>
            <Description>Requires UBL version 2.3</Description>
            <Value type="FIXED">2.3</Value>
        </Element>

        <Element>
            <Term>cbc:CustomizationID</Term>
            <Name>Specification identification</Name>
            <Description>An identification of the specification containing the total set of rules regarding semantic content, cardinalities and business rules to which the data contained in the instance document conforms.</Description>
            <DataType>Identifier</DataType>
            <Value type="FIXED">urn:www.cenbii.eu:transaction:biitrdm084:ver3.0:extended:urn:fdc:peppol.eu:2020:pracc:t007:ver1.0</Value>
        </Element>

        <Element>
            <Term>cbc:ProfileID</Term>
            <Name>Business process type identifier</Name>
            <Description>Identifies the business process context in which the transaction appears. It enables the buyer to process the document in an appropriate way.</Description>
            <DataType>Identifier</DataType>
            <Value type="FIXED">urn:fdc:peppol.eu:2020:pracc:p004:01:1.0</Value>
        </Element>

        <Element>
            <Term>cbc:ID</Term>
            <Name>Call for tender identifier</Name>
            <Description>Identifies the call for tender as issued by the contracting body. Must be expressed as a UUID</Description>
            <DataType>Identifier</DataType>

            <Attribute>
                <Term>schemeURI</Term>
                <Name>Scheme URI</Name>
                <Value type="FIXED">urn:uuid</Value>
            </Attribute>
        </Element>

        <Element>
            <Term>cbc:ContractFolderID</Term>
            <Name>Reference number</Name>
            <Description>An identifier that is specified by the buyer and used as a reference number for all documents in the procurement process. It is also known as procurement project identifier, procurement reference number or contract folder identifier.</Description>
            <DataType>Identifier</DataType>
            <Reference type="RULE">BII3-T83-R004</Reference>
        </Element>

        <Element>
            <Term>cbc:IssueDate</Term>
            <Name>Call for tender issue date</Name>
            <Description>The date on which the call for tender is issued.</Description>
            <DataType>Date</DataType>
            <Reference type="RULE">BII3-T83-R005</Reference>
        </Element>

        <Element cardinality="1..1">
            <Term>cbc:IssueTime</Term>
            <Name>Call for tender issue time</Name>
            <Description>The time when the call for tender is issued. The issue time must have granularity of seconds, and include time zone information</Description>
            <DataType>Time</DataType>
        </Element>

        <Element>
            <Term>cbc:VersionID</Term>
            <Name>Call for tender version</Name>
            <Description>Identifies the version of the call for tenders.</Description>
            <DataType>Identifier</DataType>
        </Element>

        <Element>
            <Term>cac:RequestorParty</Term>
            <Name>Requestor body</Name>

            <Element>
                <Term>cac:Party</Term>
                <Name>Party information</Name>

                <Element>
                    <Term>cbc:EndpointID</Term>
                    <Name>The economic operator electronic address identifier</Name>
                    <Description>Electronic address of the economic operator.</Description>
                    <DataType>Identifier</DataType>

                    <Attribute>
                        <Term>schemeID</Term>
                        <Name>Scheme identifier</Name>
                    </Attribute>
                </Element>

                <Element>
                    <Term>cac:PartyIdentification</Term>
                    <Name>Party identification</Name>

                    <Element>
                        <Term>cbc:ID</Term>
                        <Name>Economic operator identifier</Name>
                        <Description>The economic operator</Description>
                        <DataType>Identifier</DataType>

                        <Attribute>
                            <Term>schemeID</Term>
                            <Name>Scheme identifier</Name>
                        </Attribute>
                    </Element>
                </Element>

                <Element cardinality="0..1">
                    <Term>cac:PartyName</Term>
                    <Name>Party name</Name>
                    <Element>
                        <Term>cbc:Name</Term>
                        <Name>Economic operator name</Name>
                        <Description>The name of the economic operator.</Description>
                        <DataType>Text</DataType>
                    </Element>
                </Element>
            </Element>
        </Element>

        <Element>
            <Term>cac:ResponderParty</Term>
            <Name>Responder body</Name>

            <Element>
                <Term>cac:Party</Term>
                <Name>Party information</Name>

                <Element>
                    <Term>cbc:EndpointID</Term>
                    <Name>Contracting body electronic address identifier</Name>
                    <Description>Electronic address of the contracting body.</Description>
                    <DataType>Identifier</DataType>

                    <Attribute>
                        <Term>schemeID</Term>
                        <Name>Scheme identifier</Name>
                    </Attribute>
                </Element>

                <Element>
                    <Term>cac:PartyIdentification</Term>
                    <Name>Party identification</Name>

                    <Element>
                        <Term>cbc:ID</Term>
                        <Name>Contracting body identifier</Name>
                        <Description>The national identifier of a contracting body as it is legally registered (e.g. VAT identification, such as KBO)</Description>
                        <DataType>Identifier</DataType>

                        <Attribute>
                            <Term>schemeID</Term>
                            <Name>Scheme identifier</Name>
                        </Attribute>
                    </Element>
                </Element>

                <Element cardinality="0..1">
                    <Term>cac:PartyName</Term>
                    <Name>Party name</Name>
                    <Element>
                        <Term>cbc:Name</Term>
                        <Name>Contracting body name</Name>
                        <Description>The name of the contracting body as it is registered.</Description>
                        <DataType>Text</DataType>
                    </Element>
                </Element>
            </Element>
        </Element>

        <Element cardinality="0..n">
            <Term>cac:AdditionalDocumentReference</Term>
            <Name>Additional document reference</Name>
            <Description>Additional document reference are used for different document references, required document, provided document.
                To reference several types, the additional document should be repeated for each type.</Description>

            <Element>
                <Term>cbc:ID</Term>
                <Name>Document identifier</Name>
                <Description>Identification of the document reference</Description>
                <DataType>Identifier</DataType>
            </Element>

            <Element cardinality="0..1">
                <Term>cbc:IssueDate</Term>
                <Name>Document issue date</Name>
                <Description>Date when the referred document was issued.</Description>
                <DataType>Date</DataType>
            </Element>
        </Element>
    </Document>
</Structure>
